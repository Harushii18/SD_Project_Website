<p id="notice"><%= notice %></p>

<h1>Course Specialties</h1>

<table class="table">
  <thead>
    <tr>
      <th>Course ID</th>
      <th>Specialty ID</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @course_specialties.each do |course_specialty| %>
      <tr>
        <td><%= course_specialty.Course_id %></td>
        <td><%= course_specialty.Specialty_id %></td>
        <td><%= link_to 'Show', course_specialty %></td>
        <td><%= link_to 'Edit', edit_course_specialty_path(course_specialty) %></td>
        <td><%= link_to 'Destroy', course_specialty, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

