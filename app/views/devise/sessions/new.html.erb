<link rel="stylesheet" type="text/css" href = "../assets/Wits Health Web.css">
<div class = "wits_logo">
    <img width="160" src = "../assets/wits logo.png" alt = "Wits Logo">
</div>
<div class="title" >
    <h1>WITS</h1>
    <h2>UNIVERSITY</h2>
    <h2>HEALTH APP</h2>
</div>

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="field">
    <%= f.email_field :email, placeholder: 'Email', autofocus: true, autocomplete: "email" %>
  </div>

  <div class="field">
    <%= f.password_field :password, placeholder: 'Password', autocomplete: "current-password" %>
  </div>

  <div class= "action">
    <%= f.submit "Sign in", class: "button", type: "submit" %>
  </div>
<% end %>

<%= render "devise/shared/links" %>
